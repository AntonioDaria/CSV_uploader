<%= flash[:notice] %>
<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th>Author</th>
      <th>Date published</th>
      <th>Publisher name</th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.id %></td>
        <td><%= book.title %></td>
        <td><%= book.author %></td>
        <td><%= book.date_published %></td>
        <td><%= book.publisher %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'Add file', home_index_url %>
